//- Example Item -> [{title, text, icon}]
mixin threeBoxTextIcon(items)
  // Three Box Text Icon
  +appTable() 
    tbody 
      tr 
        +colSeparator()   
        +boxTextIcon(items[0].title, items[0].text, items[0].icon, "176")
        +colSeparator("12")
        +boxTextIcon(items[1].title, items[1].text, items[1].icon, "176")
        +colSeparator("12")
        +boxTextIcon(items[2].title, items[2].text, items[2].icon, "176")
        +colSeparator()
